alias bmain="cargo build"
alias rmain="cargo run"

alias bsub="cargo build --package submission"
alias rsub="cargo run --package submission"

alias sand="cargo run --example sandbox"

function rsample() {
  ./bundle.sh
  bat test/sample-${1}.in
  bat test/sample-${1}.out
  cargo run --package submission < test/sample-${1}.in
}
