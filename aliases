alias bmain="cargo build"
alias rmain="cargo run"

alias bsub="./bundle.sh && cargo build --package submission"
alias rsub="./bundle.sh && cargo run --package submission"

alias sand="cargo run --example sandbox"

alias clipsub="./bundle.sh && cat submission/submission.rs | clip"

function rsample() {
  ./bundle.sh
  bat test/sample-${1}.in
  bat test/sample-${1}.out
  cargo run --package submission < test/sample-${1}.in
}
